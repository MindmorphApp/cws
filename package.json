{
	"name": "copingwithschizophrenia",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "(svelte-kit sync || echo 'svelte-kit sync failed, continuing') && husky && husky install",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"test:e2e": "playwright test"
	},
	"lint-staged": {
		"**/*.{js,ts,svelte,json,css,md}": [
			"prettier --write"
		]
	},
	"devDependencies": {
		"@chromatic-com/storybook": "^3",
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@playwright/test": "^1.49.1",
		"@storybook/addon-essentials": "^8.6.9",
		"@storybook/addon-svelte-csf": "^5.0.0-next.0",
		"@storybook/blocks": "^8.6.9",
		"@storybook/experimental-addon-test": "^8.6.9",
		"@storybook/svelte": "^8.6.9",
		"@storybook/sveltekit": "^8.6.9",
		"@storybook/test": "^8.6.9",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.20.2",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.7",
		"@vitest/browser": "^3.0.9",
		"@vitest/coverage-v8": "^3.0.9",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"husky": "^8.0.0",
		"jsdom": "^26.0.0",
		"lint-staged": "^15.5.0",
		"playwright": "^1.51.1",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"storybook": "^8.6.9",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.0.0",
		"vitest": "^3.0.9"
	},
	"dependencies": {
		"svelte-i18n": "^4.0.1"
	}
}
